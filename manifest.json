{
    "name": "Smartbook Solver",
    "description": "A chrome extention to solve your boring smartbook for an Easy A in the background",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {},
    "content_scripts":[{
        "matches": ["<all_urls>"],
        "js":["contentScript.js"],
        "run_at": "document_end" 
    }],
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ]
}
